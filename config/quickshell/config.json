{
    "appearance": {
        "extraBackgroundTint": true,
        "fakeScreenRounding": 2,
        "palette": {
            "type": "auto"
        },
        "transparency": false,
        "wallpaperTheming": {
            "enableAppsAndShell": true,
            "enableQtApps": true,
            "enableTerminal": true
        }
    },
    "audio": {
        "protection": {
            "enable": true,
            "maxAllowed": 90,
            "maxAllowedIncrease": 10
        }
    },
    "background": {
        "clockX": -500,
        "clockY": -500,
        "fixedClockPosition": false,
        "parallax": {
            "enableSidebar": true,
            "enableWorkspace": true,
            "workspaceZoom": 1.07
        },
        "thumbnailPath": "",
        "wallpaperPath": ""
    },
    "bar": {
        "autoHide": {
            "enable": false,
            "pushWindows": false,
            "showWhenPressingSuper": {
                "delay": 140,
                "enable": true
            }
        },
        "borderless": false,
        "bottom": false,
        "cornerStyle": 0,
        "resources": {
            "alwaysShowCpu": false,
            "alwaysShowSwap": true
        },
        "screenList": [
        ],
        "showBackground": true,
        "topLeftIcon": "spark",
        "tray": {
            "monochromeIcons": true
        },
        "utilButtons": {
            "showColorPicker": false,
            "showDarkModeToggle": true,
            "showKeyboardToggle": true,
            "showMicToggle": false,
            "showPerformanceProfileToggle": false,
            "showScreenSnip": true
        },
        "verbose": true,
        "weather": {
            "city": "",
            "enable": false,
            "enableGPS": true,
            "fetchInterval": 10,
            "useUSCS": false
        },
        "workspaces": {
            "alwaysShowNumbers": false,
            "monochromeIcons": true,
            "showAppIcons": true,
            "showNumberDelay": 300,
            "shown": 10
        }
    },
    "battery": {
        "automaticSuspend": true,
        "critical": 5,
        "low": 20,
        "suspend": 3
    },
    "dock": {
        "enable": false,
        "height": 60,
        "hoverRegionHeight": 2,
        "hoverToReveal": true,
        "ignoredAppRegexes": [
        ],
        "monochromeIcons": true,
        "pinnedApps": [
            "org.kde.dolphin",
            "kitty", 
            "zen",
            "code-oss",
            "burpsuite",
            "steam",
            "lutris",
            "discord"
        ],
        "pinnedOnStartup": false
    },
    "hacks": {
        "arbitraryRaceConditionDelay": 20
    },
    "interactions": {
        "scrolling": {
            "fasterTouchpadScroll": true,
            "mouseScrollDeltaThreshold": 120,
            "mouseScrollFactor": 120,
            "touchpadScrollFactor": 450
        }
    },
    "language": {
        "engine": "es_CO"
    },
    "light": {
        "night": {
            "automatic": true,
            "colorTemperature": 5000,
            "from": "19:00",
            "to": "06:30"
        }
    },
    "media": {
        "filterDuplicatePlayers": true
    },
    "osd": {
        "timeout": 1000
    },
    "osk": {
        "layout": "qwerty_full",
        "pinnedOnStartup": false
    },
    "overview": {
        "columns": 5,
        "enable": true,
        "rows": 2,
        "scale": 0.18
    },
    "screenshotTool": {
        "showContentRegions": true
    },
    "time": {
        "dateFormat": "ddd, dd/MM",
        "format": "hh:mm"
    },
    "windows": {
        "centerTitle": true,
        "showTitlebar": true
    }
}
